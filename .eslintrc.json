{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "no-shadow": "off"
  }
}
